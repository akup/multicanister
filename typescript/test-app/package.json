{
  "name": "test-app",
  "version": "1.0.0",
  "description": "Simple TypeScript Express REST service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "esbuild": "esbuild src/index.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/index.js",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@dfinity/agent": "2.3.0",
    "@dfinity/identity": "2.4.1",
    "@dfinity/utils": "^2.14.0",
    "esbuild": "^0.20.2",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/node": "22.13.10",
    "prettier": "3.5.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "keywords": [
    "typescript",
    "express",
    "rest",
    "api"
  ],
  "author": "",
  "license": "MIT"
}