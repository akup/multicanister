{
  "canisters": {
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    },
    "factory": {
      "build": "cargo build --target wasm32-unknown-unknown --package factory --release",
      "candid": "canisters/factory/can.did",
      "wasm": "target/wasm32-unknown-unknown/release/factory.wasm",
      "type": "custom",
      "optimize": "cycles"
    },
    "hello": {
      "build": "cargo build --target wasm32-unknown-unknown --package hello --release",
      "candid": "canisters/hello/can.did",
      "wasm": "target/wasm32-unknown-unknown/release/hello.wasm",
      "type": "custom",
      "optimize": "cycles",
      "gzip": true
    },
    "sns_root": {
      "type": "custom",
      "candid": "canisters/sns/did/sns-root-canister.wasm.did",
      "wasm": "canisters/sns/sns-root-canister.wasm",
      "init_args_file": "canisters/sns/args/sns-root-canister.wasm.idl"
    },
    "sns_governance": {
      "type": "custom",
      "candid": "canisters/sns/did/sns-governance-canister.wasm.did",
      "wasm": "canisters/sns/sns-governance-canister.wasm",
      "init_args_file": "canisters/sns/args/sns-governance-canister.wasm.idl"

    },
    "sns_ledger": {
      "type": "custom",
      "candid": "canisters/sns/did/ic-icrc1-ledger.wasm.did",
      "wasm": "canisters/sns/ic-icrc1-ledger.wasm",
      "init_args_file": "canisters/sns/args/ic-icrc1-ledger.wasm.idl"
    },
    "sns_swap": {
      "type": "custom",
      "candid": "canisters/sns/did/sns-swap-canister.wasm.did",
      "wasm": "canisters/sns/sns-swap-canister.wasm",
      "init_args_file": "canisters/sns/args/sns-swap-canister.wasm.idl"
    },
    "sns_index": {
      "type": "custom",
      "candid": "canisters/sns/did/ic-icrc1-index-ng.wasm.did",
      "wasm": "canisters/sns/ic-icrc1-index-ng.wasm",
      "init_args_file": "canisters/sns/args/ic-icrc1-index-ng.wasm.idl"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
